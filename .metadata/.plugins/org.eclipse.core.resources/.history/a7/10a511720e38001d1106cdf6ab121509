import java.util.*;

public class Analyzerr {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		String x;
		x = sc.next();
		Data num = new Data();
		while(!(x.equals("Q"))){
			String s = sc.next();
			
			num.getSum(Double.parseDouble(s));
			System.out.print(num.getAverage());
			
		}
		
		

	}

}
