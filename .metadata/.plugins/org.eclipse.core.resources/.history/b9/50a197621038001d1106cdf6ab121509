
public class Data {
	private static int cnt;
	private double sum;
	private double maxi;
	
	private static int cntOfNumbers = 0;
	

	
	Data(){
		//this.cnt = 0;
		this.sum = 0;
		this.maxi = 0;
	}

	

	
	public void getSum(double num) {
		sum += num;
		cnt++;
	}
	
	
	
	public double getAverage() {
		return sum / cnt;
	}
	
	
	
	
	
	

}
