import java.util.*;

public class Analyzerr {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		double ans = 0;
		while(sc.hasNextDouble()){
			Data num = new Data();
			String x;
			x = sc.next();
			Data.getSum(Double.parseDouble(x));
			ans = num.getAverage();
			
		}
		System.out.print(ans);
		

	}

}
